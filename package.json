{
  "name": "anubis-db",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "t-node": "node --no-warnings --experimental-specifier-resolution=node --loader ts-node/esm ",
    "start": "yarn t-node src/start.ts",
    "node-prod": "node --experimental-specifier-resolution=node --no-warnings",
    "serve": "yarn build && yarn node-prod ./dist/start.js",
    "build": "swc ./src -d dist --config-file .swcrc --include-dotfiles --copy-files"
  },
  "dependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.218",
    "@types/express": "^4.17.13",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "body-parser": "1.20.0",
    "compression": "^1.7.4",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "express": "4.18.1",
    "express-rate-limit": "6.5.1",
    "helmet": "5.1.1",
    "is-valid-domain": "^0.1.6",
    "jade": "1.11.0",
    "lodash-es": "^4.17.21",
    "mongoose": "6.4.6",
    "morgan": "1.10.0",
    "prettier": "^2.7.1",
    "rotating-file-stream": "^3.0.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
